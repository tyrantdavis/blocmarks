<!-- <h1><%#= @user.username %></h1> -->
<h3>My Bookmarks</h3>

<div class="row">
  <%= render @topics %>
</div>

<hr>

<h2> My Likes </h2>

<div class="row">
  <% @liked_bookmarks.each do |like| %>
  <div class="col-sm-3">
    <div class="well bookmark_box">
      <%= render partial: 'bookmarks/thumbnail', locals: { bookmark: like.bookmark } %>

    </div>
    <div class="button_placement">
    <%= render partial: 'likes/like', locals: { bookmark: like.bookmark } %>
    </div>
  </div>
  <% end %>
</div>
